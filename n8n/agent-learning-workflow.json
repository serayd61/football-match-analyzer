    {
      "parameters": {
        "jsCode": "// Agent performans Ã¶zeti oluÅŸtur\nconst performance = $input.all();\n\nconst summary = {\n  timestamp: new Date().toISOString(),\n  agents: performance.map(p => ({\n    name: p.json.agent_name,\n    accuracy: parseFloat(p.json.recent_match_result_accuracy) || 0,\n    weight: parseFloat(p.json.current_weight) || 1.0,\n    totalMatches: p.json.total_matches || 0,\n    trend: p.json.trend_direction || 'stable'\n  })),\n  totalAgents: performance.length\n};\n\nreturn {\n  json: summary\n};"
      },
      "id": "format-summary",
      "name": "Format Summary",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1050, 200]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  agent_name,\n  COUNT(*) as total_predictions,\n  COUNT(*) FILTER (WHERE match_result_correct = TRUE) as correct_mr,\n  COUNT(*) FILTER (WHERE over_under_correct = TRUE) as correct_ou,\n  COUNT(*) FILTER (WHERE btts_correct = TRUE) as correct_btts,\n  AVG(CASE WHEN match_result_correct IS NOT NULL THEN CASE WHEN match_result_correct THEN 100 ELSE 0 END END) as mr_accuracy,\n  AVG(CASE WHEN over_under_correct IS NOT NULL THEN CASE WHEN over_under_correct THEN 100 ELSE 0 END END) as ou_accuracy,\n  AVG(CASE WHEN btts_correct IS NOT NULL THEN CASE WHEN btts_correct THEN 100 ELSE 0 END END) as btts_accuracy\nFROM agent_predictions\nWHERE settled_at IS NOT NULL\n  AND settled_at >= NOW() - INTERVAL '7 days'\nGROUP BY agent_name\nORDER BY mr_accuracy DESC",
        "options": {}
      },
